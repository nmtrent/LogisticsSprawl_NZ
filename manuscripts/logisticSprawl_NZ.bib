	Bdsk-Url-1 = {https://doi.org/10.1068/a281261}}

@article{Rivera_etal2014,
	abstract = {Governments around the world are investing significant resources in the development of logistics clusters. This paper develops a methodology for identifying them and applies it to answer several lingering questions in the context of the US. It contributes to a more general debate in the general industrial clusters literature: while many authors see industrial clusters growing, others see them dispersing. To answer this and related questions in the context of logistics clusters the paper first analyzes the prevalence of such clusters using a two-index methodology to identify clusters in the US. Evidence of increasing concentration of the logistics industry in clusters in the US over time is tested and documented. In addition, some evidence that logistics activities in counties inside clusters show higher growth than counties outside clusters is found. {\textcopyright} 2013 Elsevier Ltd.},
	author = {Rivera, Liliana and Sheffi, Yossi and Welsch, Roy},
	doi = {10.1016/j.tra.2013.11.009},
	file = {:Users/nadiaviljoen/Documents/Research{\_}Repositories/085/docs/literature/Rivera{\_}etal2014.pdf:pdf},
	issn = {09658564},
	journal = {Transportation Research Part A: Policy and Practice},
	keywords = {Agglomeration,Cluster,Concentration,Logistics,Supply chain},
	pages = {222--238},
	publisher = {Elsevier},
	title = {{Logistics agglomeration in the US}},
	volume = {59},
