	keywords = {Centrographic method,Characteristics of metropolitan areas,Data collection procedure,Linear regression,Logistics sprawl,Warehouse},
	month = {nov},
	number = {22},
	pages = {9779},
	title = {{Influence of Characteristics of Metropolitan Areas on the Logistics Sprawl: A Case Study for Metropolitan Areas of the State of Paran{\'{a}} (Brazil)}},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85096573046{\&}doi=10.3390{\%}2Fsu12229779{\&}partnerID=40{\&}md5=920307652644078d058682cd6c0c7b70 https://www.mdpi.com/2071-1050/12/22/9779},
