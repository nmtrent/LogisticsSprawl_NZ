@article{Yuan2018,
	abstract = {With the rapid spatial expansion of the warehousing industry in major metropolitan areas, environmental impacts associated with warehousing activities have been growing in the recent decades. This study focuses on the disproportionate distribution of warehousing facilities in disadvantaged neighborhoods and discusses how the disparities result from the interactions between various socioeconomic processes. From the perspective of environmental justice, warehousing-related environmental hazards affect the spatial relationship between warehouses and local communities. The changing factors in the firm location choice of warehousing facilities and the housing location choice of disadvantaged population jointly lead to the environmental justice problem in warehousing location.},
