	mendeley-tags = {2003-2016,64 metropolitan areas,Chow test,USA,ZIP code level,absolute centrality,central,centrography,employees proxy volume,freight-shipment data,govt register,longitudinal,national,population,population proxy freight demand,relative centrality,supply chain partners,time-series},
